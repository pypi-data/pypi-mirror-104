include "searchDomains" "filterRepeated" 
include "wpscan.mist"
#import "./wpscan.py"

function save(r) {
    print("Wordpress found at", r.target_url)
    fields = strSplit(r.target_url,"/")
    writeToFile(r, "{fields[2]}.json")
}

searchDomains(%domain) => filterRepeated(False) => wpscan() => save()
